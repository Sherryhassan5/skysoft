{
    "builds":[
        {
            "src":"skysoft_website/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime":"python3.12"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"skysoft_website/wsgi.py"
        }
    ]
}